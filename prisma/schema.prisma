generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @db.Char(26)
  username    String @unique @db.VarChar(50)
  displayName String @db.VarChar(100)
  password    String @db.VarChar(255)
  roles       String @db.VarChar(50)
}
